# .env.integration.local.example - Copy to .env.integration.local for local overrides

# Local Database Override (if running different port)
# DATABASE_URL=postgresql://test:test@localhost:5434/echo_integration_test

# Local Echo Control Override (if running different port)
# ECHO_CONTROL_URL=http://localhost:3002

# Local JWT Secret Override (for local development)
# JWT_SECRET=your-local-jwt-secret-here

# Local Clerk Keys Override (if you have test Clerk keys)
# CLERK_SECRET_KEY=your-local-clerk-secret
# CLERK_PUBLISHABLE_KEY=your-local-clerk-publishable

# Local Browser Testing (disable headless for debugging)
# HEADLESS=false
# BROWSER_TIMEOUT=0