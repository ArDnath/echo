{
  "name": "echo-example",
  "version": "1.0.0",
  "description": "Example CLI application for Echo platform",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "echo-example": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/cli.js",
    "dev": "tsc --watch",
    "link": "pnpm run build && npm link",
    "unlink": "npm unlink -g echo-example",
    "lint": "pnpx eslint src/**/*.ts",
    "lint:fix": "pnpx eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "echo",
    "typescript",
    "cli",
    "sdk",
    "api"
  ],
  "author": "Echo Team",
  "license": "MIT",
  "dependencies": {
    "@merit-systems/echo-typescript-sdk": "1.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "inquirer": "^9.2.0",
    "open": "^9.1.0",
    "openai": "^5.11.0"
  },
  "files": [
    "dist/**/*"
  ]
}
